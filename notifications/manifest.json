{
  "name": "消息通知",
  "manifest_version": 2,
  "version": "1.0.0",
  "description":
    "定时显示消息通知",
  "icons": {"16": "img/icon_16.png", "48": "img/icon_48.png", "128": "img/icon_128.png"},
  "permissions": [
    "notifications"
