{
  "manifest_version": 2,
  "name": "Hello World",
  "version": "1.0.0",
  "description": "Welcome to chrome extension world",
  "homepage_url": "http://liujinkai.com/",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    },
    "default_popup": "popup.html",
    "default_title": "Click to open new world"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"],
      "run_at": "document_end"
    }
  ]
}
