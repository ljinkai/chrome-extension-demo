{
  "name": "一键生成网页截图",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "将当前浏览的网页一键保存为图片",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab", "storage", "unlimitedStorage" ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
